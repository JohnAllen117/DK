<table class="table">
  <thead>
    <tr>
      <th>First Name </th>
      <th>Last Name </th>
      <th>Email</th>
      <th>Beta Number </th>
      <th>Role </th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.beta_number %></td>
        <td><%= user.role.humanize %></td>
      </tr>
    <% end %>
  </tbody>
</table>
